{
  "name": "report",
  "plural": "reports",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp" : {
      "createdAt" : "createdOn",
      "updatedAt" : "updatedOn",
      "required" : false,
      "validateUpsert": true,
      "silenceWarnings": true
    }
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "defaultFn": "now"
    },
    "location": {
      "type": "geopoint",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
